{
  "name": "rsuite-datepicker",
  "version": "0.3.0",
  "description": "React Datepicker Component",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "test": "npm run lint && mocha --compilers js:babel-core/register --colors test/*_spec.js",
    "build": "rm -rf ./lib && babel ./src --out-dir ./lib && cp -R src/less lib",
    "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3100 --host 0.0.0.0 ",
    "docs": "rm -rf assets &&  NODE_ENV=production webpack ",
    "cp:docs": "cp -rf ./assets/* ../rsuite.github.io/rsuite-datepicker/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsuite/rsuite-datepicker.git"
  },
  "keywords": [
    "react-component",
    "react",
    "datepicker"
  ],
  "author": "A2ZH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rsuite/rsuite-datepicker/issues"
  },
  "homepage": "https://github.com/rsuite/rsuite-datepicker#readme",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^6.4.1",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.5.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "html-loader": "^0.4.5",
    "mocha": "^3.1.0",
    "react-addons-test-utils": "^15.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "markdown-loader": "2.0.0",
    "markdownloader": "1.0.5",
    "rsuite": "0.0.24",
    "rsuite-theme": "0.3.22",
    "url-loader": "^0.5.7",
    "file-loader": "^0.10.1",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "peerDependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "dependencies": {
    "moment": ">=2.15.2"
  }
}
